{
    "name": "voipgrid",
    "manifest_version": 2,
    "author": "VoIPGRID",
    "version": "1.0",
    "description": "Chrome plugin voor klik&bel, statusupdate en wachtrij monitoring",
    "background": {
        "scripts": ["assets/js/jquery.js", "background.js"]
    },
    "browser_action": {
        "default_icon": {
          "38": "assets/img/call-gray.png"
        },
        "default_title": "VoIPGRID",
        "default_popup": "login.html"
    },
    "permissions": [
        "https://client.voys.nl/"
    ]
}
